    MathCorrect = 0
}

local display = GamesTab:AddParagraph("ðŸ“‹ Your Scores", "")
local function updateDisplay()
    display:Set(string.format("â€¢ Clicks: %d\nâ€¢ Reaction Time: %s\nâ€¢ Guess Tries: %s\nâ€¢ Math Correct: %d",
        scoreData.Clicks,
        scoreData.FastestReaction ~= math.huge and scoreData.FastestReaction .. "ms" or "--",
        scoreData.GuessTries ~= math.huge and scoreData.GuessTries or "--",
        scoreData.MathCorrect))
end
