local musicLibrary = {
    ["🌮 It's Raining Tacos"] = "rbxassetid://142376088",
    ["🎧 Chill Lofi"] = "rbxassetid://1846458016",
    ["🎤 Rap Beat"] = "rbxassetid://133988484250262",
